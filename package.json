{
  "name": "advanced-poe-filter-parser",
  "version": "0.9.0",
  "keywords": [
    "path-of-exile",
    "filter-of-kalandra",
    "peg"
  ],
  "homepage": "https://github.com/isuke/advanced-poe-filter-parser",
  "repository": "git@github.com:isuke/advanced-poe-filter-parser.git",
  "main": "lib/parser.cjs",
  "module": "lib/parser.mjs",
  "types": "lib/parser.d.ts",
  "author": "isuke <isuke770@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "lint": "eslint test/**/*.js",
    "lint:fix": "eslint --fix test/**/*.js",
    "prettier": "prettier --write --log-level=warn '**/*.js'",
    "test": "ava --verbose",
    "build:c": "pegjs -o lib/parser.cjs src/advanced-poe-filter.pegjs --format=commonjs",
    "build:m": "pegjs -o lib/parser.mjs src/advanced-poe-filter.pegjs --format=es",
    "build": "npm run --silent build:c; npm rum --silent build:m",
    "predeploy": "npm run build",
    "deploy": "npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "5.3.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "outdent": "0.8.0",
    "pegjs": "0.11.0-master.b7b87ea",
    "prettier": "3.0.3",
    "sinon": "15.2.0"
  }
}
